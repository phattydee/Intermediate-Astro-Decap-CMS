---
import BaseLayout from "@layouts/BaseLayout.astro";
import Hero329 from "@components/Hero/Hero329.astro";
import Hero330 from "@components/Hero/Hero330.astro";
import Hero331 from "@components/Hero/Hero331.astro";
import SBS332 from "@components/SideBySide/SBS332.astro";
import SBSR333 from "@components/SideBySide/SBSR333.astro";
import SBS334 from "@components/SideBySide/SBS334.astro";
import SBS335 from "@components/SideBySide/SBS335.astro";
import SBS336 from "@components/SideBySide/SBS336.astro";
import SBSR337 from "@components/SideBySide/SBSR337.astro";
import SBS338 from "@components/SideBySide/SBS338.astro";
import SBS339 from "@components/SideBySide/SBS339.astro";
import Stats340 from "@components/Stats/Stats340.astro";
import Services341_344 from "@components/Services/Services341_344.astro";
import Services342 from "@components/Services/Services342.astro";
import Services343 from "@components/Services/Services343.astro";
import Services345 from "@components/Services/Services345.astro";
import MeetUs347 from "@components/MeetOurTeam/MeetUs347.astro";
import MeetOurTeam348 from "@components/MeetOurTeam/MeetOurTeam348.astro";
import FAQ349 from "@components/FAQ/FAQ349.astro";
import FAQ351 from "@components/FAQ/FAQ351.astro";
import FAQ352 from "@components/FAQ/FAQ352.astro";
import Reviews353 from "@components/Reviews/Reviews353.astro";
import Reviews399 from "@components/Reviews/Reviews399.astro";
import Pricing357 from "@components/Pricing/Pricing357.astro";
import Hero358 from "@components/Hero/Hero358.astro";
import Hero359 from "@components/Hero/Hero359.astro";
import Hero361 from "@components/Hero/Hero361.astro";
import Hero362 from "@components/Hero/Hero362.astro";
import Hero363 from "@components/Hero/Hero363.astro";
import Hero364 from "@components/Hero/Hero364.astro";
import Hero365 from "@components/Hero/Hero365.astro";
import Services366 from "@components/Services/Services366.astro";
import Services367 from "@components/Services/Services367.astro";
import Services368 from "@components/Services/Services368.astro";
import Hero369 from "@components/Hero/Hero369.astro";
import Hero370 from "@components/Hero/Hero370.astro";
import SBSR371 from "@components/SideBySideReverse/SBSR371.astro";
import Hero372 from "@components/Hero/Hero372.astro";
import Hero373 from "@components/Hero/Hero373.astro";
import Gallery404 from "@components/Gallery/Gallery404.astro";
import SBS411_412 from "@components/SideBySide/SBS411_412.astro";
import Footer1104 from "@components/Footer/Footer1104.astro";
import Contact1105 from "@components/Contact/Contact1105.astro";
---

<BaseLayout
    title="Stitch Preview"
    description="Preview page for ported CodeStitch components."
>
    <div style="padding-top: 100px;">
        <h1 style="text-align: center; margin-bottom: 50px;">
            Batch 1 Preview
        </h1>

        <section style="margin-bottom: 100px;">
            <h2 style="text-align: center; background: #eee; padding: 10px;">
                Hero 329
            </h2>
            <Hero329 />
        </section>

        <section style="margin-bottom: 100px;">
            <h2 style="text-align: center; background: #eee; padding: 10px;">
                Hero 330
            </h2>
            <Hero330 />
        </section>

        <section style="margin-bottom: 100px;">
            <h2 style="text-align: center; background: #eee; padding: 10px;">
                Hero 331 (Hero + Services)
            </h2>
            <Hero331 />
        </section>

        <section style="margin-bottom: 100px;">
            <h2 style="text-align: center; background: #eee; padding: 10px;">
                SBS 332 (Side By Side)
            </h2>
            <SBS332 />
        </section>

        <section style="margin-bottom: 100px;">
            <h2 style="text-align: center; background: #eee; padding: 10px;">
                SBSR 333 (Side By Side Reverse)
            </h2>
            <SBSR333 />
        </section>
        <section style="margin-bottom: 100px;">
            <h2 style="text-align: center; background: #eee; padding: 10px;">
                Batch 2
            </h2>

            <h3 style="text-align: center; padding: 10px;">
                SBS 334 (Side By Side + Reverse)
            </h3>
            <SBS334 />

            <h3 style="text-align: center; padding: 10px;">
                SBS 335 (Side By Side + Reverse + Triplet)
            </h3>
            <SBS335 />

            <h3 style="text-align: center; padding: 10px;">
                SBS 336 (Side By Side)
            </h3>
            <SBS336 />

            <h3 style="text-align: center; padding: 10px;">
                SBSR 337 (Side By Side Reverse)
            </h3>
            <SBSR337 />

            <h3 style="text-align: center; padding: 10px;">
                SBS 338 (Side By Side + Reverse)
            </h3>
            <SBS338 />
        </section>

        <section style="margin-bottom: 250px;">
            <h2 style="text-align: center; background: #eee; padding: 10px;">
                Batch 3
            </h2>

            <h3 style="text-align: center; padding: 10px;">
                SBS 339 (Reverse Triplet)
            </h3>
            <SBS339 variation="standard" />
            <div style="margin-top: 50px;">
                <SBS339 variation="reverse" />
            </div>
            <div style="margin-top: 50px;">
                <SBS339 variation="triplet" />
            </div>

            <h3 style="text-align: center; padding: 10px; margin-top: 100px;">
                Stats 340
            </h3>
            <Stats340 />

<h3 style="text-align: center; padding: 10px; margin-top: 100px;">
                Services 341 & 344 (Images vs Icons)
            </h3>
            <Services341_344 type="images" />
            <div style="margin-top: 50px;">
                <Services341_344 type="icons" />
            </div>

            <h3 style="text-align: center; padding: 10px; margin-top: 100px;">
                Services 342 (6 Card)
            </h3>
            <Services342 />

            <h3 style="text-align: center; padding: 10px; margin-top: 100px;">
                Services 343 (4 Card)
            </h3>
            <Services343 />

            <h3 style="text-align: center; padding: 10px; margin-top: 100px;">
                Services 345 (4 Card with Icons + Button)
            </h3>
            <Services345 />

            <h3 style="text-align: center; padding: 10px; margin-top: 100px;">
                Meet Our Team 347 (4 Card Team + Animated Bubbles)
            </h3>
            <MeetUs347 />
            
            <h2 style="text-align: center; background: #eee; padding: 10px; margin-top: 100px;">
                Meet Our Team 348 (3 Card Team + Bubbles)
            </h2>
            <MeetOurTeam348 />
            
            <h2 style="text-align: center; background: #eee; padding: 10px; margin-top: 100px;">
                FAQ 349 (Side By Side)
            </h2>
            <FAQ349 />
            
            <h2 style="text-align: center; background: #eee; padding: 10px; margin-top: 100px;">
                FAQ 351 (Standard)
            </h2>
            <FAQ351 />
            
            <h2 style="text-align: center; background: #eee; padding: 10px; margin-top: 100px;">
                Reviews 399 (3 Card Testimonials)
            </h2>
            <Reviews399 />

            <h3 style="text-align: center; padding: 10px; margin-top: 100px;">
                SBS 411 & 412 (About)
            </h3>
            <SBS411_412 variation="standard" />
            <div style="margin-top: 50px;">
                <SBS411_412 variation="reverse" showSlant={true} />
            </div>

            <h3 style="text-align: center; padding: 10px; margin-top: 100px;">
                Contact 1105
            </h3>
            <Contact1105 />

            <h3 style="text-align: center; padding: 10px; margin-top: 100px;">
                Footer 1104
            </h3>
            <Footer1104 />
        </section>

        <section style="margin-bottom: 250px;">
            <h2 style="text-align: center; background: #eee; padding: 10px;">
                Batch 4 - Additional Stitches
            </h2>

            <h3 style="text-align: center; padding: 10px; margin-top: 100px;">
                FAQ 352 (Side By Side with Images)
            </h3>
            <FAQ352 />

            <h3 style="text-align: center; padding: 10px; margin-top: 100px;">
                Reviews 353 (2 Card Testimonials)
            </h3>
            <Reviews353 />

            <h3 style="text-align: center; padding: 10px; margin-top: 100px;">
                Pricing 357 (3 Card Pricing)
            </h3>
            <Pricing357 />

            <h3 style="text-align: center; padding: 10px; margin-top: 100px;">
                Hero 358 (Left Aligned)
            </h3>
            <Hero358 />

            <h3 style="text-align: center; padding: 10px; margin-top: 100px;">
                Hero 359 (Why Choose)
            </h3>
            <Hero359 />

            <h3 style="text-align: center; padding: 10px; margin-top: 100px;">
                Hero 361 (Left Aligned)
            </h3>
            <Hero361 />

            <h3 style="text-align: center; padding: 10px; margin-top: 100px;">
                Hero 362 (Left Aligned)
            </h3>
            <Hero362 />

            <h3 style="text-align: center; padding: 10px; margin-top: 100px;">
                Hero 363 (Left Aligned)
            </h3>
            <Hero363 />

            <h3 style="text-align: center; padding: 10px; margin-top: 100px;">
                Hero 364 (Left Aligned)
            </h3>
            <Hero364 />

            <h3 style="text-align: center; padding: 10px; margin-top: 100px;">
                Hero 365 (Left Aligned)
            </h3>
            <Hero365 />

            <h3 style="text-align: center; padding: 10px; margin-top: 100px;">
                Services 366 (3 Card)
            </h3>
            <Services366 />

            <h3 style="text-align: center; padding: 10px; margin-top: 100px;">
                Services 367 (3 Card with Icons)
            </h3>
            <Services367 />

            <h3 style="text-align: center; padding: 10px; margin-top: 100px;">
                Services 368 (4 Card)
            </h3>
            <Services368 />

            <h3 style="text-align: center; padding: 10px; margin-top: 100px;">
                Hero 369 (Left Aligned)
            </h3>
            <Hero369 />

            <h3 style="text-align: center; padding: 10px; margin-top: 100px;">
                Hero 370 (Left Aligned)
            </h3>
            <Hero370 />

            <h3 style="text-align: center; padding: 10px; margin-top: 100px;">
                SBSR 371 (Side By Side Reverse with Images)
            </h3>
            <SBSR371 />

            <h3 style="text-align: center; padding: 10px; margin-top: 100px;">
                Hero 372 (Left Aligned)
            </h3>
            <Hero372 />

            <h3 style="text-align: center; padding: 10px; margin-top: 100px;">
                Hero 373 (Left Aligned)
            </h3>
            <Hero373 />

            <h3 style="text-align: center; padding: 10px; margin-top: 100px;">
                Gallery 404 (Filterable Gallery)
            </h3>
            <Gallery404 />
        </section>
    </div>
</BaseLayout>
